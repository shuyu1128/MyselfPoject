@charset "utf-8";
/* CSS Document */

body{ font-family:Arial, Helvetica, sans-serif;}
*{ padding:0px; margin:0px;}

li{ list-style:none;}
a{ text-decoration:none;}
img{ border:0px;}


.fl{ float:left;}
.fr{ float:right;}
.clear{ clear:both;}
.clearfix:after{ content:"."; height:0px; clear:both; display:block; visibility:hidden;}
.clearfix{zoom:1;}

.wid{ width:1150px; margin:0 auto;}
.wid1{ width:960px;margin:0 auto;}


