.nav {
  margin-left: 80px;
}

.nav ul {}

.nav ul li {
  width: 130px;
  text-align: center;
  font-size: 18px;
  line-height: 65px;
  float: left;
  position: relative;
  z-index: +1
}

.nav ul li a {
  color: #FFF;
  display: block;
  background: url(../img/shuxian.gif) no-repeat right;
}

.nav ul li a:hover {
  /*background:#FFF; color:#999999;*/
}

/*.nav ul li .box{ width:130px; background:#FFF;padding: 10px 0px}
.nav ul li .box p{font-size:14px; line-height:30px; }
.nav ul li .box p a{ background:none; color:#0168B3; }*/
.one_nav:hover .box {
  display: block;
}

.nav ul li ul.box {
  width: 130px;
  background: #FFF;
  padding: 10px 0px;
  display: none;
}

.nav ul li ul.box li {
  font-size: 14px;
  line-height: 30px;
  position: relative;
}

.nav ul li ul.box li a {
  background: none;
  color: #0168B3;
  display: block;
  text-decoration: none;
}

.nav ul li ul.box li a:hover {
  display: block;
  background: #e5e5e5;
}

.nav ul li ul.box li ol {
  width: 130px;
  background: #F9F9F9;
  position: absolute;
  left: 130px;
  top: 0px;
}

.nav ul li ul.box li ol li {
  font-size: 14px;
  line-height: 30px;
}

.nav ul li ul.box li ol li a {
  color: #0168B3;
}

.section {
  min-height: 340px;
}


.banner {
  width: 1440;
  margin: 0 auto;
  position: relative;
}

.banner img {
  display: block;
  margin: 0 auto;
}

.banner ul {
  position: absolute;
  right: 247px;
  bottom: 20px;
}

.banner ul li {
  float: left;
  margin-right: 15px;
  width: 20px;
  height: 20px;
}

.banner ul li a {
  display: block;
}

.banner ul li a:hover {
  background: url(../img/yuanquan1.gif) no-repeat center;
}

.content {
